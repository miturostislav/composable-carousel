{
  "name": "accessible-carousel",
  "version": "1.0.0",
  "description": "lightweight accessible carousel",
  "scripts": {
    "build": "webpack --env.production",
    "dev": "webpack --env.development --progress --colors --watch",
    "eslint": "eslint --fix src/**/*.js",
    "prettier": "prettier --write src/**/*.js",
    "pretty-quick": "pretty-quick --staged --pattern '**/*.*js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mitu Rostislav",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "husky": "^1.3.1",
    "path": "^0.12.7",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint && npm run pretty-quick"
    }
  }
}
